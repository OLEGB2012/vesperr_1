<p id="notice"><%= notice %></p>

<h1>About Us Sections</h1>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th>Left side</th>
      <th>Right side</th>
      <th>Happy clients counter</th>
      <th>Projects counter</th>
      <th>Years counter</th>
      <th>Awards counter</th>
      <th>Happy clients text</th>
      <th>Projects text</th>
      <th>Years text</th>
      <th>Awards text</th>
      <th>Happy clients class</th>
      <th>Projects class</th>
      <th>Years class</th>
      <th>Awards class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_us_sections.each do |about_us_section| %>
      <tr>
        <td><%= about_us_section.section %></td>
        <td><%= about_us_section.left_side %></td>
        <td><%= about_us_section.right_side %></td>
        <td><%= about_us_section.happy_clients_counter %></td>
        <td><%= about_us_section.projects_counter %></td>
        <td><%= about_us_section.years_counter %></td>
        <td><%= about_us_section.awards_counter %></td>
        <td><%= about_us_section.happy_clients_text %></td>
        <td><%= about_us_section.projects_text %></td>
        <td><%= about_us_section.years_text %></td>
        <td><%= about_us_section.awards_text %></td>
        <td><%= about_us_section.happy_clients_class %></td>
        <td><%= about_us_section.projects_class %></td>
        <td><%= about_us_section.years_class %></td>
        <td><%= about_us_section.awards_class %></td>
        <td><%= link_to 'Show', about_us_section %></td>
        <td><%= link_to 'Edit', edit_about_us_section_path(about_us_section) %></td>
        <td><%= link_to 'Destroy', about_us_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About Us Section', new_about_us_section_path %>
