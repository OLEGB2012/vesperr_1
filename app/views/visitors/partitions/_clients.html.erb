<!-- ======= Clients Section ======= -->
<section id="<%= anchor_link(section_name) %>" class="clients clients">
  <div class="container">
    <div class="row">
      <% @clients.each do |item| %>
        <div class="col-lg-2 col-md-4 col-6">
          <%= image_tag("clients/client-#{item.id}.png", class: "img-fluid", alt: "", data: {aos: "zoom-in"}) %>
        </div>
      <% end %>
    </div>
  </div>
</section> <!-- End Clients Section -->